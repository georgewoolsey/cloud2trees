    lasR (>= 0.13.1),
    leafR (>= 0.3.5),
    lidR,
    lifecycle,
    magrittr,
    patchwork,
    posterior,
    purrr,
    randomForest,
    readr,
    rlas,
    scales,
    sf,
    stats,
    stringr,
    terra,
    tidyr,
    tidyselect,
    tools,
    TreeLS (>= 2.0.5),
    utils,
    viridis
